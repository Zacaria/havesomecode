{
  "name": "i18node",
  "version": "1.0.0",
  "description": "i18n try with nodejs",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "gulp",
    "deploy": "npm run clean && cd docs && npm run build && ls && npm run copyAssets && npm run surge",
    "surge": "surge -d $npm_package_config_deploy_domain -p docs/_static",
    "clean": "rimraf docs/_static"
  },
  "repository": {
    "type": "git",
    "url": "Zacaria"
  },
  "keywords": [
    "i18n",
    "nodejs",
    "express",
    "jade"
  ],
  "author": "Zacaria",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.3",
    "i18next": "^1.10.4",
    "jade": "^1.11.0",
    "pug": "^2.0.3",
    "puppeteer": "^1.6.1"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "gulp": "^3.9.1",
    "gulp-exec": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sass": "^4.0.1",
    "rimraf": "^2.6.2"
  },
  "config": {
    "deploy_domain": "havesome-resume.surge.sh"
  }
}
